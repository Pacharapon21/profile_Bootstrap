{
  "version": 2,
  "builds": [
    { "src": "manage.py", "use": "@vercel/python", "config": { "zeroConfig": true } }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/.*", "dest": "/api/index.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "profile_django.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
